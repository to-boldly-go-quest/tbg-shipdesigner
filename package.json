  "version": "1.0.0",
  "name": "tbg-shipbuilder",
  "scripts": {
    "build": "vue-cli-service build",
    "deploy-gh-pages": "git subtree push --prefix dist origin gh-pages",
    "import-parts": "node bin/import-parts.js",
    "lint": "vue-cli-service lint",
